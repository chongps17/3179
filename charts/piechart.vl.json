{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {"text": "Percentage of forested area in Australian fires by state (2019-20)"},
  "data": {
    "url": "https://raw.githubusercontent.com/chongps17/3179/refs/heads/main/data/fire_forest_data.csv"
  },
"transform": [
    {"filter": "datum.percent_forest > 0"}
  ],
  "mark": {"type": "arc", "outerRadius": 100},
  "encoding": {
    "theta": {
      "field": "percent_forest",
      "type": "quantitative",
      "title": "Percent Forest"
    },
    "color": {
      "field": "state",
      "type": "nominal",
      "title": "State"
    },
    "tooltip": [
      {"field": "state", "title": "State"},
      {"field": "percent_forest", "type": "quantitative", "title": "% Forest"}
      
    ]
  },
  "view": {"stroke": null}
}